{

  "compilerOptions": {
    "jsx": "react",
    "target": "es6",
    "module": "Commonjs",
    "outDir": "./dist",
    "baseUrl": "./src",
    "strict": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "include": ["src", "src/importSvg"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
